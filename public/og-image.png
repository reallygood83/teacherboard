<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Gradient (Chanel Luxury Theme) -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#000000;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#D4AF37;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#F4D03F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D4AF37;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bgGradient)"/>

  <!-- Decorative Lines -->
  <line x1="0" y1="80" x2="1200" y2="80" stroke="url(#goldGradient)" stroke-width="2" opacity="0.3"/>
  <line x1="0" y1="550" x2="1200" y2="550" stroke="url(#goldGradient)" stroke-width="2" opacity="0.3"/>

  <!-- Logo Icon (T in a box - Neo-Brutalism style) -->
  <rect x="100" y="200" width="120" height="120" fill="#FFEB3B" stroke="#000000" stroke-width="4"/>
  <rect x="104" y="204" width="120" height="120" fill="#FFEB3B" stroke="#000000" stroke-width="4"/>
  <text x="164" y="285" font-family="Arial, sans-serif" font-size="72" font-weight="900" fill="#000000" text-anchor="middle">T</text>

  <!-- Main Title -->
  <text x="250" y="265" font-family="Arial, sans-serif" font-size="64" font-weight="900" fill="#FFFFFF" letter-spacing="2">
    TeaBoard
  </text>

  <!-- Subtitle Korean -->
  <text x="250" y="320" font-family="Arial, sans-serif" font-size="36" font-weight="600" fill="#D4AF37">
    우리 학급 홈페이지
  </text>

  <!-- Feature Tags (Neo-Brutalism style) -->
  <g transform="translate(100, 380)">
    <!-- Tag 1: AI 도구 -->
    <rect x="0" y="0" width="180" height="60" fill="#00E5FF" stroke="#000000" stroke-width="3"/>
    <rect x="4" y="4" width="180" height="60" fill="#00E5FF" stroke="#000000" stroke-width="3"/>
    <text x="94" y="42" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#000000" text-anchor="middle">🤖 AI 도구</text>

    <!-- Tag 2: 수업 관리 -->
    <rect x="200" y="0" width="200" height="60" fill="#FF6B9D" stroke="#000000" stroke-width="3"/>
    <rect x="204" y="4" width="200" height="60" fill="#FF6B9D" stroke="#000000" stroke-width="3"/>
    <text x="304" y="42" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#000000" text-anchor="middle">📚 수업 관리</text>

    <!-- Tag 3: 학생 관리 -->
    <rect x="420" y="0" width="200" height="60" fill="#00E676" stroke="#000000" stroke-width="3"/>
    <rect x="424" y="4" width="200" height="60" fill="#00E676" stroke="#000000" stroke-width="3"/>
    <text x="524" y="42" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#000000" text-anchor="middle">👥 학생 관리</text>

    <!-- Tag 4: YouTube -->
    <rect x="640" y="0" width="180" height="60" fill="#C6FF00" stroke="#000000" stroke-width="3"/>
    <rect x="644" y="4" width="180" height="60" fill="#C6FF00" stroke="#000000" stroke-width="3"/>
    <text x="734" y="42" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#000000" text-anchor="middle">🎬 YouTube</text>
  </g>

  <!-- Bottom Tagline -->
  <text x="600" y="510" font-family="Arial, sans-serif" font-size="28" font-weight="500" fill="#FFFFFF" text-anchor="middle" opacity="0.9">
    선생님을 위한 올인원 디지털 교실 플랫폼
  </text>

  <!-- URL -->
  <text x="600" y="560" font-family="Arial, sans-serif" font-size="24" font-weight="600" fill="#D4AF37" text-anchor="middle">
    teaboard.link
  </text>
</svg>
